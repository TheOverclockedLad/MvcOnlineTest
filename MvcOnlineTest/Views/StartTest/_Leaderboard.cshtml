@model IList<_Leaderboard>

<div class="h1 text-center">Leaderboard</div>

<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Score</th>
    </tr>

    @foreach (_Leaderboard item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.FirstName)</td>
            <td>@Html.DisplayFor(modelItem => item.LastName)</td>
            <td>@Html.DisplayFor(modelItem => item.Score)</td>
        </tr>
    }
</table>